# Compliance Monitor Agent Configuration
# Copy this file to .env and configure your settings

# Server endpoint for reporting compliance data
CM_ENDPOINT=http://localhost:3000/api/report

# API key for authentication with the server
CM_API_KEY=your_api_key_here

# Interval settings (in minutes)
CM_MIN_INTERVAL=15
CM_MAX_INTERVAL=60

# Agent behavior settings
CM_ONCE=false
CM_DRY_RUN=false
CM_VERBOSE=false
CM_INSECURE=false
